  "repository": "https://github.com/MuffinKing-jpeg/wg-api-lib.git",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.19",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
